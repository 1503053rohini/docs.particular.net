<?xml version="1.0" encoding="utf-8" ?>
<configuration>
  <configSections>
    <section name="Logging" type="NServiceBus.Config.Logging, NServiceBus.Core" />
    <section name="MasterNodeConfig" type="NServiceBus.Config.MasterNodeConfig, NServiceBus.Core" />
    <section name="UnicastBusConfig" type="NServiceBus.Config.UnicastBusConfig, NServiceBus.Core"/>
  </configSections>
  <Logging Threshold="Info" />
  <!--startcode WorkerNameToUseWhileTestingconfig-->
  <appSettings>
    <add
      key="NServiceBus/Distributor/WorkerNameToUseWhileTesting"
      value="Sample.Scaleout.Worker1" />
  </appSettings>
  <!--endcode-->
  <MasterNodeConfig Node="localhost"/>
  <UnicastBusConfig
    DistributorControlAddress="Sample.Scaleout.Server.Distributor.control@localhost"
    DistributorDataAddress="Sample.Scaleout.Server@localhost"/>
</configuration>
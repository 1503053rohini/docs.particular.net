<?xml version="1.0" encoding="utf-8"?>
<!--startcode workerConfig-->
<configuration>
  <configSections>
    <section name="MasterNodeConfig" 
             type="NServiceBus.Config.MasterNodeConfig, NServiceBus.Core" />
    <section name="UnicastBusConfig" 
             type="NServiceBus.Config.UnicastBusConfig, NServiceBus.Core" />
  </configSections>
  <appSettings>
    <add 
      key="NServiceBus/Distributor/WorkerNameToUseWhileTesting" 
      value="Sample.Scaleout.Worker1" />
  </appSettings>
  <MasterNodeConfig Node="localhost" />
  <UnicastBusConfig 
    DistributorControlAddress="Sample.Scaleout.Server.Distributor.control@localhost" 
    DistributorDataAddress="Sample.Scaleout.Server@localhost"/>
</configuration>
<!--endcode-->
<?xml version="1.0" encoding="utf-8"?>
<configuration>
  <configSections>
    <section name="UnicastBusConfig" type="NServiceBus.Config.UnicastBusConfig, NServiceBus.Core" />
  </configSections>
  <!--startcode SenderMessageMappings-->
  <UnicastBusConfig>
    <MessageEndpointMappings>
        <add Assembly="Messages" Endpoint="Samples.SqlServer.MultiInstanceReceiver" />
    </MessageEndpointMappings>
  </UnicastBusConfig>
  <!--endcode-->
  <connectionStrings>
    <add name="NServiceBus/Transport"
         connectionString="Data Source=.\SQLEXPRESS;Initial Catalog=not-used;Integrated Security=True"/>
  </connectionStrings>
</configuration>